[
        { rabbit, [
                { loopback_users, [ ] },
                { tcp_listeners, [ 5672 ] },
                { ssl_listeners, [ ] },
                { hipe_compile, false }
        ] },

	{rabbitmq_mqtt, [{default_user,     <<"guest">>},
			  {default_pass,     <<"guest">>},
			  {allow_anonymous,  true},
			  {vhost,            <<"/">>},
			  {exchange,         <<"amq.topic">>},
			  {subscription_ttl, 1800000},
			  {prefetch,         10},
			  {ssl_listeners,    []},
			  %% Default MQTT with TLS port is 8883
			  %% {ssl_listeners,    [8883]}
			  {tcp_listeners,    [1883]},
			  {tcp_listen_options, [{backlog,   128},
						{nodelay,   true}]}]},

        { rabbitmq_management, [ { listener, [
                { port, 15672 },
                { ssl, false }
        ] } ] }
].
